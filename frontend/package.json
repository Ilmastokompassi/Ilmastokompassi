        "react-router-dom": "^6.15.0",
        "swr": "^2.2.4"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.22.20",
        "@babel/preset-react": "^7.22.15",
        "@testing-library/jest-dom": "^6.1.3",
        "@testing-library/react": "^14.0.0",
        "@types/fetch-mock": "^7.3.6",
